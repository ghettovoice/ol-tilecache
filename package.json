{
  "name": "ol-tilecache",
  "version": "3.0.1",
  "description": "OpenLayers tile url function to load tile seeded with TileCache url scheme",
  "main": "dist/bundle.js",
  "module": "dist/bundle.es.js",
  "scripts": {
    "build-umd": "cross-env BABEL_ENV=umd NODE_ENV=production rollup -c build/rollup.umd.config.js",
    "build-es": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c build/rollup.es.config.js",
    "build": "npm run clean && npm run build-es && npm run build-umd",
    "clean": "rm -rf ./dist",
    "start": "webpack-dev-server --config=build/webpack.dev.config.js",
    "test": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "coveralls": "cat test/unit/coverage/lcov.info | coveralls"
  },
  "homepage": "https://github.com/ghettovoice/ol-tilecache",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghettovoice/ol-tilecache.git"
  },
  "keywords": [
    "ol",
    "openlayers",
    "tilecache",
    "ol-tilecache",
    "tile url function",
    "map",
    "mapping",
    "ol plugins",
    "openlayers plugins"
  ],
  "author": "Vladimir Vershinin <ghettovoice@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghettovoice/ol-tilecache/issues"
  },
  "dependencies": {},
  "peerDependencies": {
    "ol": "^5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^2.0.4",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
    "mocha-phantomjs": "^4.1.0",
    "ol": "^5.0.2",
    "phantomjs-prebuilt": "^2.1.16",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "webpack-notifier": "^1.6.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src"
  ],
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
